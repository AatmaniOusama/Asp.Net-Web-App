                WHEN @rbEnroler  = -1  THEN 1 
                  WHEN  @rbEnroler  = 1 THEN case WHEN  dbo.UsersBioData.BioData is not null THEN 1
               ELSE 0
               END
              WHEN  @rbEnroler  = 0  THEN case WHEN   dbo.UsersBioData.BioData is null THEN 1
               ELSE 0
               END
              ELSE 0
       END 

 AND 1 = CASE  
              WHEN @matricule  IS NULL THEN 1 
              WHEN dbo.Users.Matricule  LIKE  +@matricule+ '%'    THEN 1 
              ELSE 0 
