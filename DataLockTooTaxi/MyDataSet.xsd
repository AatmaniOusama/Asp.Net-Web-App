                      <Parameter AllowDbNull="true" AutogeneratedName="" DataSourceName="" DbType="String" Direction="Input" ParameterName="numPermis" Precision="0" ProviderType="NVarChar" Scale="0" Size="0" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="" DataSourceName="" DbType="String" Direction="Input" ParameterName="immatriculation" Precision="0" ProviderType="NVarChar" Scale="0" Size="0" SourceColumn="Plaque" SourceColumnNullMapping="false" SourceVersion="Current" />
