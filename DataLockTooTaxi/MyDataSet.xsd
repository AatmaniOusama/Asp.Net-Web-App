              WHEN dbo.Users.Prenom  LIKE  +@prenom+ '%'    THEN 1 
              ELSE 0 
       END 
  AND  1 = CASE  
              WHEN @numPermis  IS NULL THEN 1 
              WHEN dbo.Users.NumBadge  LIKE  +@numPermis+ '%'    THEN 1 
              ELSE 0 
       END
  AND  1 = CASE  
              WHEN @rbautoriseValue  IS NULL THEN 1 
              WHEN dbo.Users.FlagAutorise  =@rbautoriseValue THEN 1 
              ELSE 0 
       END

  AND  1 = CASE  
              WHEN @rbCoderValue IS NULL THEN 1 
              WHEN dbo.Users.BadgeEncoded=@rbCoderValue     THEN 1 
              ELSE 0 
       END                                                 

</CommandText>
